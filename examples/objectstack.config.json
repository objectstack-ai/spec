{
  "version": "1.0.0",
  "plugins": [
    {
      "name": "objectstack-objectql",
      "enabled": true,
      "options": {
        "env": "production",
        "debug": false
      }
    },
    {
      "name": "objectstack-driver-memory",
      "enabled": true,
      "options": {
        "maxRecords": 10000
      }
    },
    {
      "name": "objectstack-data",
      "enabled": true,
      "options": {
        "enableHooks": true,
        "enableValidation": true
      }
    },
    {
      "name": "objectstack-server",
      "enabled": true,
      "options": {
        "port": 3000,
        "cors": true
      }
    },
    {
      "name": "objectstack-api",
      "enabled": true,
      "options": {
        "prefix": "/api/v1"
      }
    },
    {
      "name": "objectstack-flow",
      "enabled": false,
      "options": {
        "maxConcurrentFlows": 10
      }
    },
    {
      "name": "objectstack-ui",
      "enabled": true,
      "options": {
        "theme": "default",
        "enableDarkMode": true
      }
    }
  ]
}
