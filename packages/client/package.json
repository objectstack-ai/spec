    "@objectstack/plugin-hono-server": "workspace:*",
    "@objectstack/plugin-msw": "workspace:*",
    "@objectstack/runtime": "workspace:*",
    "msw": "^2.12.9",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
