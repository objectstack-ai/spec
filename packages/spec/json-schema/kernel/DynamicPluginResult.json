{
  "$ref": "#/definitions/DynamicPluginResult",
  "definitions": {
    "DynamicPluginResult": {
      "type": "object",
      "properties": {
        "success": {
          "type": "boolean"
        },
        "operation": {
          "type": "string",
          "enum": [
            "load",
            "unload",
            "reload",
            "enable",
            "disable"
          ],
          "description": "Runtime plugin operation type"
        },
        "pluginId": {
          "type": "string"
        },
        "durationMs": {
          "type": "integer",
          "minimum": 0
        },
        "version": {
          "type": "string"
        },
        "error": {
          "type": "object",
          "properties": {
            "code": {
              "type": "string",
              "description": "Machine-readable error code"
            },
            "message": {
              "type": "string",
              "description": "Human-readable error message"
            },
            "details": {
              "type": "object",
              "additionalProperties": {}
            }
          },
          "required": [
            "code",
            "message"
          ],
          "additionalProperties": false
        },
        "warnings": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": [
        "success",
        "operation",
        "pluginId"
      ],
      "additionalProperties": false,
      "description": "Result of a dynamic plugin operation"
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#"
}