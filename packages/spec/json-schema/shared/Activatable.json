        },
        "activatedAt": {
          "type": "string",
          "format": "date-time",
          "description": "When the entity was last activated"
        },
        "deactivatedAt": {
          "type": "string",
          "format": "date-time",
          "description": "When the entity was last deactivated"
        }
      },
      "additionalProperties": false
    }
  },
